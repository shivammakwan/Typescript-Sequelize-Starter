{
  "name": "Typescript-Sequelize-Starter",
  "version": "1.0.0",
  "description": "Typescript-Sequelize-Starter",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm run build && npm run watch",
    "serve": "nodemon --config nodemon-config.json --inspect dist/app.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build": "rimraf dist && npm run build-ts",
    "watch": "concurrently -k -p \"[{name}]\" \" npm run watch-ts \" \" npm run serve\"",
    "debug": "npm run build && npm run watch-debug",
    "watch-debug": "npm run watch-ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "html": "find ./app -name '*.html' -type f -exec cp --parents {} ./dist \\;"
  },
  "author": "shivam makwan<shivammakwan124@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/sequelize": "^4.28.4",
    "concurrently": "^4.1.2",
    "nodemailer": "^6.3.0",
    "nodemon": "^1.19.2",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/boom": "^7.3.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/crypto-js": "^3.1.43",
    "@types/dotenv": "^6.1.1",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/morgan": "^1.7.37",
    "@types/nodemailer": "^6.2.1",
    "@types/sequelize": "^4.28.4",
    "body-parser": "^1.19.0",
    "boom": "^7.3.0",
    "continuation-local-storage": "^3.2.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql2": "^1.7.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "sequelize": "^5.19.1",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0"
  }
}
